<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AddOrders">

    <View
            android:layout_width="417dp"
            android:layout_height="585dp" android:id="@+id/section6"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="#F2F2F2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="1.0" app:layout_constraintTop_toTopOf="parent"/>
    <Button
            android:text="Add Order"
            android:layout_width="268dp"
            android:layout_height="62dp" android:id="@+id/logIn3"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.496" app:layout_constraintBottom_toBottomOf="@+id/pageBackground"
            app:layout_constraintVertical_bias="0.935" android:textColor="#FFFFFF"
            android:background="@drawable/orange_corner"
            android:paddingLeft="30dp" android:paddingRight="30dp" android:textAllCaps="false" android:textSize="16sp"
            android:layout_marginTop="624dp"/>
    <View
            android:layout_width="417dp"
            android:layout_height="200dp" android:id="@+id/section7"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/rounder_corner"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintHorizontal_bias="0.533"/>
    <TextView
            android:layout_width="299dp"
            android:layout_height="42dp"
            android:text="Add Order"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.201"
            android:textColor="#000000" android:id="@+id/textView12" android:textSize="25sp"
            android:fontFamily="sans-serif-condensed"
            android:textAlignment="center"/>
    <ImageButton
            android:layout_width="38dp"
            android:layout_height="36dp" app:srcCompat="@drawable/ic_back" android:id="@+id/imageButton5"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/section1"
            app:layout_constraintHorizontal_bias="0.067" app:layout_constraintVertical_bias="0.057"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="660dp"/>
    <ImageView
            android:layout_width="70dp"
            android:layout_height="90dp" app:srcCompat="@drawable/logo" android:id="@+id/logo5"
            app:layout_constraintBottom_toBottomOf="@+id/section1"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.419"
            android:contentDescription="logo" android:layout_marginTop="32dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.498"/>
    <TextView
            android:text="Customer Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView20"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.153"
            app:layout_constraintVertical_bias="0.327"/>
    <TextView
            android:text="Customer Phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView21"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.154"
            app:layout_constraintVertical_bias="0.456"/>
    <TextView
            android:text="Time Orderd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView22"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.141"
            app:layout_constraintVertical_bias="0.585"/>
    <TextView
            android:text="Order Status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView13"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.141"
            app:layout_constraintVertical_bias="0.73"/>
    <EditText
            android:layout_width="319dp"
            android:layout_height="51dp"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:drawablePadding="12dp"
            android:id="@+id/editTextTextPersonName7"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/section7" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.108" android:textColorLink="#FF5722"
            android:textColorHighlight="#FF5722" android:drawableStart="@drawable/ic_person"
    />
    <EditText
            android:layout_width="319dp"
            android:layout_height="51dp"
            android:inputType="textPersonName"
            android:drawablePadding="12dp"
            android:text="Name"
            android:ems="10"
            android:id="@+id/editTextTextPersonName"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/section7" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.706" android:textColorLink="#FF5722"
            android:textColorHighlight="#FF5722" android:drawableStart="@drawable/ic_order"/>
    <EditText
            android:layout_width="319dp"
            android:layout_height="51dp"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/editTextPhone"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/section7"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.3" app:layout_constraintHorizontal_bias="0.51"
            android:textColorHighlight="#FF5722" android:textColorLink="#FF5722"
            android:drawableStart="@drawable/ic_phone"/>
    <EditText
            android:layout_width="319dp"
            android:layout_height="51dp"
            android:inputType="time"
            android:ems="10"
            android:drawablePadding="12dp"
            android:id="@+id/editTextTime"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/section6" app:layout_constraintBottom_toBottomOf="@+id/section6"
            app:layout_constraintHorizontal_bias="0.51" app:layout_constraintVertical_bias="0.543"
            android:textColorLink="#FF5722" android:textColorHighlight="#FF5722"
            android:drawableStart="@drawable/ic_time"/>
</androidx.constraintlayout.widget.ConstraintLayout>
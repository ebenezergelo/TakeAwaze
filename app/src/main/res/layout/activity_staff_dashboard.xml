<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".StaffDashboard" android:id="@+id/staffDashboard">

    <View
            android:layout_width="417dp"
            android:layout_height="585dp" android:id="@+id/section4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="1.0" app:layout_constraintTop_toTopOf="parent"/>
    <View
            android:layout_width="409dp"
            android:layout_height="237dp" android:id="@+id/section5"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/rounder_corner"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="Click here to add new orders."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/goAdd"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="@+id/section5"
            app:layout_constraintVertical_bias="0.363"
            android:textColor="#F3663A"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintBottom_toTopOf="@+id/scrollView2"
            app:layout_constraintTop_toBottomOf="@+id/textView10"/>
    <TextView
            android:layout_width="299dp"
            android:layout_height="42dp"
            android:text="Admin Dashboard"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.201"
            android:textColor="#000000" android:id="@+id/textView10" android:textSize="25sp"
            android:fontFamily="sans-serif-condensed"
            android:textAlignment="center"/>
    <ImageView
            android:layout_width="70dp"
            android:layout_height="90dp" app:srcCompat="@drawable/logo" android:id="@+id/logo4"
            app:layout_constraintBottom_toBottomOf="@+id/section1"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.419"
            android:contentDescription="logo" android:layout_marginTop="32dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.498"/>
    <ScrollView
            android:layout_width="414dp"
            android:layout_height="509dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/section" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="1.0"
            android:id="@+id/scrollView2">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical" android:id="@+id/scroll" android:layout_margin="10dp"
                      android:layout_marginStart="10dp" android:layout_marginTop="20dp"
                      android:layout_marginRight="10dp" android:layout_marginBottom="30dp"
                      android:layout_marginLeft="10dp"/>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>
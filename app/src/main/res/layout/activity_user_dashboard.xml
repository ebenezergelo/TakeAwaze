<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UserDashboard">

    <View
            android:layout_width="417dp"
            android:layout_height="585dp" android:id="@+id/section3"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="#F2F2F2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="1.0" app:layout_constraintTop_toTopOf="parent"/>
    <View
            android:layout_width="417dp"
            android:layout_height="200dp" android:id="@+id/section"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/rounder_corner"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintHorizontal_bias="0.533"/>
    <TextView
            android:layout_width="299dp"
            android:layout_height="42dp"
            android:text="My Orders"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.201"
            android:textColor="#000000" android:id="@+id/textView7" android:textSize="25sp"
            android:fontFamily="sans-serif-condensed"
            android:textAlignment="center"/>
    <ImageButton
            android:layout_width="38dp"
            android:layout_height="36dp" app:srcCompat="@drawable/ic_back" android:id="@+id/imageButton4"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/section1"
            app:layout_constraintHorizontal_bias="0.067" app:layout_constraintVertical_bias="0.057"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="660dp"/>
    <ScrollView
            android:layout_width="414dp"
            android:layout_height="533dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/section" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                      android:orientation="vertical"/>
    </ScrollView>
    <ImageView
            android:layout_width="138dp"
            android:layout_height="154dp" app:srcCompat="@drawable/food" android:id="@+id/imageView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.027"/>
</androidx.constraintlayout.widget.ConstraintLayout>
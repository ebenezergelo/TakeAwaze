<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UserDashboard">

    <View
            android:layout_width="417dp"
            android:layout_height="585dp" android:id="@+id/section3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="1.0" app:layout_constraintTop_toTopOf="parent"/>
    <View
            android:layout_width="412dp"
            android:layout_height="244dp" android:id="@+id/section"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/rounder_corner"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintHorizontal_bias="0.533"/>
    <TextView
            android:text="Click on the order to update rating!"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/goAdd2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="@+id/section5"
            android:textColor="#F3663A"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintTop_toBottomOf="@+id/textView10" android:layout_marginEnd="96dp"
            app:layout_constraintBottom_toBottomOf="@+id/section" android:layout_marginBottom="20dp"/>
    <TextView
            android:layout_width="299dp"
            android:layout_height="42dp"
            android:text="My Orders"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.201"
            android:textColor="#000000" android:id="@+id/textView7" android:textSize="25sp"
            android:fontFamily="sans-serif-condensed"
            android:textAlignment="center"/>
    <ImageView
            android:layout_width="138dp"
            android:layout_height="154dp" app:srcCompat="@drawable/food" android:id="@+id/imageView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.027"/>
    <ListView
            android:layout_width="408dp"
            android:layout_height="489dp"
            android:cacheColorHint="#00000000"
            android:divider="@drawable/separator_line"
            app:layout_constraintTop_toBottomOf="@+id/section5"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/list" android:dividerHeight="10dp"
            android:paddingTop="10dp" android:paddingBottom="10dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>